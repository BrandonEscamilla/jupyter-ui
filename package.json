  "license": "MIT",
  "private": true,
  "main": "lib/index.js",
  "files": [
    "lib",
    "style"
  ],
  "keywords": [
    "jupyter",
    "react",
    "jupyterlab"
  ],
  "scripts": {
    "start": "lerna run --parallel --scope @datalayer/jupyter-react start",
    "start:create-react-app": "lerna run --parallel --scope @datalayer/jupyter-create-react-app-example start",
    "start:docusaurus": "lerna run --parallel --scope @datalayer/jupyter-docusaurus-example start",
    "start:esbuild": "lerna run --parallel --scope @datalayer/jupyter-esbuild-example start",
    "start:lexical": "lerna run --parallel --scope @datalayer/jupyter-lexical-example start",
    "start:slate": "lerna run --parallel --scope @datalayer/jupyter-slate-example start",
    "start:prosemirror": "lerna run --parallel --scope @datalayer/jupyter-prosemirror-example start",
    "build": "lerna run build",
    "build:example": "lerna run --scope @datalayer/* --parallel build:example",
    "build:vercel": "yarn && yarn build && yarn build:example",
    "clean": "rimraf node_modules lib build",
    "deploy": "lerna run deploy",
    "literate-themes": "lerna run --parallel --scope @datalayer/jupyter-themes start",
    "editor:slate": "lerna run --parallel --scope '@datalayer/iterate' start",
    "editor:react": "lerna run --parallel --scope '@datalayer/{themes,widgets}' start",
    "test": "lerna run test"
  },
  "workspaces": {
    "packages": [
      "deprecated/*",
      "examples/*",
      "experiments/*",
      "packages/*",
      "patches/*"
    ]
  },
  "dependencies": {
  },
